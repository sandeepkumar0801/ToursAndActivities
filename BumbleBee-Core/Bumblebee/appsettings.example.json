{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_SERVER;Database=YOUR_DATABASE;Trusted_Connection=true;MultipleActiveResultSets=true;",
    "IsangoLiveDB": "Server=YOUR_SERVER;Database=YOUR_ISANGO_DATABASE;Trusted_Connection=true;MultipleActiveResultSets=true;"
  },
  "Jwt": {
    "Key": "YOUR_JWT_SECRET_KEY_HERE_MINIMUM_32_CHARACTERS",
    "Issuer": "https://yourdomain.com",
    "Audience": "https://yourdomain.com"
  },
  "AppSettings": {
    "ApiKey": "YOUR_API_KEY",
    "ApiSecret": "YOUR_API_SECRET",

    // Hop-on-Hop-off Bus Platform Domains
    "HopOnHoffOffBus": "www.hop-on-hop-off-bus.com",
    "LocalGranCanariaTour": "www.local-gran-canaria-tours.com",
    "AlhambraGranadaTour": "www.alhambra-granada-tours.com",
    "LocalDubaiTours": "www.localdubaitours.com",
    "LocalVeniceTours": "www.localvenicetours.com",
    "LocalParisTours": "www.localparistours.com",

    // Alternative API Keys for different domains
    "AlternateAPIKeyHOHO": "YOUR_HOHO_API_KEY",
    "AlternateAPIKeyLGCT": "YOUR_GRAN_CANARIA_API_KEY",
    "AlternateAPIKeyAGT": "YOUR_ALHAMBRA_API_KEY",
    "AlternateAPIKeyLDT": "YOUR_DUBAI_API_KEY",

    // BigBus Integration
    "BigBusURI": "https://api.bigbus.com/",
    "UserId": "YOUR_BIGBUS_USER_ID",
    "Password_BigBus": "YOUR_BIGBUS_PASSWORD",

    // City Sightseeing Integration
    "CitySightSeeingApiUrl": "https://api.citysightseeing.com/",
    "CitySightSeeingApiKey": "YOUR_CSS_API_KEY",

    // Other Supplier APIs
    "BaseAddressV12": "https://api.redeam.com/v12/",
    "ApiSecretKeyV12": "YOUR_REDEAM_V12_SECRET_KEY",
    "ApiKeyV12": "YOUR_REDEAM_V12_API_KEY",
    "TiqetsCertificateName": "tiqets-prod.pfx",
    "TiqetsCertificateNameCitySightSeeing": "tiqets-css.pfx",
    "BokunNotificationEmailAddressIsango": "support@hop-on-hop-off-bus.com",
    "SupportPhoneNumer": "+1-555-0123",
    "IsReservation": "false",
    "IsUncompressedStagingAPI": "false",
    "MaxParallelThreadCount": "5",
    "TicketPerPassenger": "true"
  },
  "ApplicationInsights": {
    "ConnectionString": "YOUR_APPLICATION_INSIGHTS_CONNECTION_STRING"
  }
}
